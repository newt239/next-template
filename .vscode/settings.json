{
  "editor.tabSize": 2,
  "prettier.enable": false,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.stylelint": "explicit",
    "source.fixAll.oxc": "explicit",
    "source.organizeImports": "never"
  },
  "css.validate": false,
  "biome.enabled": false,
  "files.insertFinalNewline": true,
  "oxc.enable": true
}
