{
  "name": "next-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "setup": "corepack enable pnpm",
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "oxlint --type-aware",
    "lint:fix": "oxlint --fix",
    "format": "oxfmt --check",
    "format:fix": "oxfmt",
    "typecheck": "npx tsc --noEmit",
    "codecheck": "pnpm run typecheck && pnpm run lint && pnpm run format",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@libsql/client": "0.17.0",
    "better-auth": "1.4.17",
    "drizzle-orm": "0.45.1",
    "nanoid": "5.1.6",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "server-only": "0.0.1",
    "tailwindcss-react-aria-components": "2.0.1",
    "tw-animate-css": "1.4.0",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.18",
    "@types/node": "25.1.0",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "drizzle-kit": "0.31.8",
    "lefthook": "2.0.16",
    "oxfmt": "0.26.0",
    "oxlint": "1.39.0",
    "oxlint-tsgolint": "0.11.3",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3"
  },
  "packageManager": "pnpm@10.27.0"
}
